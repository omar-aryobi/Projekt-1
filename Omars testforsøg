import numpy as np

def dataLoad(filename):
    data = np.empty([0,3])
    print(data)
    with open(filename, mode = 'r') as f:
        fl = f.readlines()
        for i in fl:
            print(i.split())
    
    return None

print(dataLoad('test.txt'))

